<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/order_Button"
        android:layout_width="89dp"
        android:layout_height="89dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="20dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:scaleType="fitCenter"
        android:src="@drawable/orders_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/basket_Button" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text="@string/coffee_icon"
        android:textColor="#E11383"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/coffee_Button"
        app:layout_constraintStart_toEndOf="@+id/coffee_Button"
        app:layout_constraintTop_toTopOf="@+id/coffee_Button" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text="@string/orders_icon"
        android:textColor="#E11383"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/order_Button"
        app:layout_constraintStart_toEndOf="@+id/order_Button"
        app:layout_constraintTop_toTopOf="@+id/order_Button" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text="@string/donuts_icon"
        android:textColor="#E11383"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/donut_Button"
        app:layout_constraintStart_toEndOf="@+id/donut_Button"
        app:layout_constraintTop_toTopOf="@+id/donut_Button" />

    <ImageButton
        android:id="@+id/donut_Button"
        android:layout_width="89dp"
        android:layout_height="89dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="20dp"
        android:adjustViewBounds="false"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:scaleType="fitCenter"
        android:src="@drawable/donut_appicon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/order_Button" />

    <ImageButton
        android:id="@+id/coffee_Button"
        android:layout_width="89dp"
        android:layout_height="89dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="20dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:scaleType="fitCenter"
        android:src="@drawable/coffee_appicon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/donut_Button" />

    <ImageButton
        android:id="@+id/basket_Button"
        android:layout_width="89dp"
        android:layout_height="89dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:scaleType="fitCenter"
        android:src="@drawable/basket_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="334dp"
        android:layout_height="205dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/coffee" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/rutgers_cafe"
        android:textAllCaps="true"
        android:textColor="#FF671F"
        android:textSize="50sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="@string/basket_icon"
        android:textColor="#E11383"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/basket_Button"
        app:layout_constraintStart_toEndOf="@+id/basket_Button"
        app:layout_constraintTop_toTopOf="@+id/basket_Button" />

</androidx.constraintlayout.widget.ConstraintLayout>
